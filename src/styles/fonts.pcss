:root {
  --ids__font: 'TT Firs Text Regular', Arial, sans-serif;
}

@font-face {
  font-family: 'TT Firs Text Regular';
  font-weight: 400;
  font-style: normal;
  font-display: swap;
  src:
    url('/fonts/TT Firs Text Regular.ttf') format('truetype');
}

@font-face {
  font-family: 'TT Firs Neue Bold';
  font-weight: 700;
  font-style: normal;
  font-display: swap;
  src:
    url('/fonts/TT Firs Neue Bold.ttf') format('truetype');
}
